[Unit]
Description=gf2-demo-api test environment
Documentation=
After=network.target
Wants=

[Service]
Type=simple
#User=
#Group=
Environment=GF_GERROR_BRIEF=true GF_GCFG_FILE=config.test.yaml
WorkingDirectory=/usr/local/gf2-demo_test
ExecStart=/usr/local/gf2-demo_test/gf2-demo-api
KillMode=process
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target
